{
	"name": "app",
	"version": "1.0.50",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"check": "bunx biome check ./src",
		"check:fix": "bunx biome check --write ./src",
		"dev": "bun --bun run --watch src/index.ts",
		"db:push": "bun src/db/migrate.ts",
		"db:migrate": "npx drizzle-kit generate:sqlite"
	},
	"dependencies": {
		"drizzle-orm": "^0.30.10",
		"elysia": "latest"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "^1.2.1",
		"drizzle-kit": "^0.20.18"
	},
	"module": "src/index.js"
}